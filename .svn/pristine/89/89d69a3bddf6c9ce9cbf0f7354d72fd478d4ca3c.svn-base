.enterpriseCredit {
  // 首页最大容器
  width: 100%;
  background: #f7f7f7;
  // 图标公共样式
  .noteLogo{
    vertical-align: middle;
    font-size:15px
  }
  // logo图标课时div
    .time {
      height: 1.55rem;
      background: #cb0a0f;
      display: flex;
      border-bottom: 1px solid #e4c6c6;
      .timeLogoDiv {
        width: 1.3rem;
        height: 100%;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
        align-items: center;
        .timeLogo {
          height: 0.83rem;
          width: 0.83rem;
        }
        p {
          font-size: 10px;
          color: #fff;
        }
        .button {
          height: 0.28rem;
          width: 0.75rem;
          background: #fec104;
          text-align: center;
          line-height: 0.28rem;
          font-size: 13px;
          color: #fff;
          box-shadow: 0px 0px 2px 1px #d85415;
          border-radius: 3px;
        }
      }
      // 课时情况DIV
      .classHour {
        flex: 1;
        padding-right: 0.39rem;
        padding-top: 0.28rem;
        display: flex;
        flex-direction: column;
        .compulsoryOrelective {
          width: 100%;
          height: 0.19rem;
          display: flex;
          justify-content: space-between;
          align-items: center;
          span {
            color: #feda0a;
            font-size: 13px;
          }
        }
        .totalHourOralreadyHour {
          flex: 1;
          padding-bottom: 0.225rem;
          display: flex;
          justify-content: space-around;
          align-items: center;
          span:nth-child(1) {
            display: flex;
            flex-direction: column;
            justify-content: space-around;
            align-items: center;
            h2 {
              font-size: 44px;
              color: #fff;
            }
            em {
              font-size: 13px;
              color: #fff;
            }
          }
          span:nth-child(2) {
            display: block;
            width: 0.02rem;
            background: #e9c8c9;
            border-radius: 10px;
            height: 100%;
          }
          span:nth-child(3) {
            display: flex;
            flex-direction: column;
            justify-content: space-around;
            align-items: center;
            h2 {
              font-size: 44px;
              color: #fff;
            }
            em {
              font-size: 13px;
              color: #fff;
            }
          }
        }
      }
      
    }
    // 笔记和心得div
    .noteOrexperience{
      height: 0.4rem;
      background:#cb0a0f ;
      display: flex;
      justify-content: space-around;
      align-items: center;
      font-size: 13px;
      color: #fff;
      padding-left: 0.25rem;
      padding-right: 0.25rem;
      box-shadow:inset 0px -12px 10px -16px #000;
      .noteLogoColor{
        color:#feda0a ;
      }
      span:nth-child(2){
        display: block;
        width: 0.02rem;
        background: #e9c8c9;
        border-radius: 10px;
        height: 0.22rem;
      }
    }
    // 必修div
    .compulsoryDiv{
      width: 100%;
      height: 1.95rem;
      background: #fff;
      border-top: 1px solid #f2f2f2;
      border-bottom: 1px solid #f2f2f2;
      display: flex;
      flex-direction: column;
      margin-bottom: 0.09rem;;
      .compulsoryHeader{
        height: 0.5rem;
        display: flex;
        justify-content: start;
        align-items: center;
        padding-left: 0.15rem;
        span:nth-child(1){
          display: block;
            width: 0.04rem;
            background: #0ca7fc;
            border-radius: 10px;
            height: 0.26rem;
            margin-right: 0.08rem;
        }
        span:nth-child(2){
          font-size: 15px;
          color: #7f7f7f;
          line-height: 0.05rem;
        }
      }
      // 完成未完成选项卡div
      .complete{
        flex: 1;
        display: flex;
        flex-direction: column;
        // 选项卡
        .tabDiv{
          height: 0.35rem;
          border-bottom: 2px solid #e7e7e7;
          display: flex;
          justify-content: center;
          align-items: center;
          span{
            width: 50%;
            height: 100%;
            display: block;
            background: #fafafa;
            text-align: center;
            line-height: 0.35rem;
            color: #b8b8b8;
            font-size: 15px;
          }
          span:nth-child(1){
            border-top: 2px solid #e7e7e7;
            border-right: 0.5px solid #e7e7e7;
          }
          span:nth-child(2){
            border-top: 2px solid #e7e7e7;
            border-left: 0.5px solid #e7e7e7;
          }
          // 选中样式
          .active{
            color: #c33c3d;
            background: #fff;
            border-top-color: #c33c3d !important;
            transition: 0.2s;
          }
        }
        // 选项卡数据
        .tabData{
          flex: 1;
          div{
            display: flex;
            justify-content: space-between;
            align-items: center;
            width: 100%;
            height: 100%;
            padding-left: 0.15rem;
            padding-right: 0.15rem;
          }
          // 未完成计划的div内容显示
          div:nth-child(1){
            display: flex;
            justify-content: space-between;
            align-items: center;
          }
          div:nth-child(1) span{
            display: block;
          }
          div:nth-child(1) span:nth-child(1){
            width: 65%;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: space-around;
            padding-top: 0.08rem;
            padding-bottom: 0.05rem;
            p{
              font-size: 15px;
              white-space: nowrap;
              overflow: hidden;
              text-overflow: ellipsis;
              font-weight: bold;
            }
            span{
              border:none;
              width: auto;
              height: auto;
              display: flex;
              justify-content: space-between;
              align-items: center;
              span{
                height: 0.11rem;
                width: 1.05rem;
                background: #eaeaea;
                border-radius: 0 20px 20px 0;
                position: relative;
                span{
                  position: absolute;
                  height: 100%;
                  border-radius: 0 20px 20px 0;
                  background: #ee2400;
                  top: 0;
                  left: 0;
                }
              }
              em{
                font-size: 13px;
                color: #ee2400;
                padding-right: 0.5rem;
              }
            }
          }
          div:nth-child(1) span:nth-child(2){
              display: block;
              width: 1.17rem;
              height: 0.63rem;
              border:1px solid #ccc;
          }
        }
        
      }
    }
  }