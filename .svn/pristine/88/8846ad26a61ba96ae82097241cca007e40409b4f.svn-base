.learningReport{background:#f8f8f8;display:flex;flex-direction:column}.learningReport .learningReportData{overflow:auto;margin-top:0.32rem}.learningReport .learningReportData #myChart{background:#fff;height:3.64rem;padding-top:0.44rem;border-bottom:1px solid #ccc}.learningReport .learningReportData .statisticsDiv{width:100%;height:0.75rem;display:flex;border-bottom:1px solid #d9d9d9;margin-bottom:0.08rem;background:#fff;justify-content:space-between;align-items:center;padding-left:0.16rem;padding-right:0.16rem}.learningReport .learningReportData .statisticsDiv span{font-size:15px;display:flex;justify-content:center;align-items:center}.learningReport .learningReportData .statisticsDiv span:nth-child(1) .iconColor{color:#cb0a0f;font-size:23px;margin-right:0.05rem}.learningReport .learningReportData .statisticsDiv span:nth-child(1) em{height:23px;line-height:25px;color:#707070}.learningReport .learningReportData .statisticsDiv span:nth-child(2){color:#000;font-weight:700}
