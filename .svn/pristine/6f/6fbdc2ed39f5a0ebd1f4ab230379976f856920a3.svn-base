.compulsoryDiv /deep/.van-loading {
  text-align: center;
}

.compulsoryDiv .hasData {
  font-size: 15px;
  text-align: center;
  padding-top: 0.2rem;
}

.compulsoryDiv .complete {
  margin-top: 0.9rem;
  padding: 0 0.1rem 0 0.1rem;
  background: #f8f8f8;
  padding-bottom: 0rem;
}

.compulsoryDiv .complete .tabDataChildren {
  font-size: 16px;
  margin-bottom: 0.2rem;
  background: #fff;
  border-radius: 0.05rem;
}

.compulsoryDiv .complete .tabDataChildren img {
  width: 100%;
  height: 100%;
  border-radius: 0.05rem;
}

.compulsoryDiv .complete .tabDataChildren .barContainer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 0.1rem;
}

.compulsoryDiv .complete .tabDataChildren .barContainer .barContainerChildren {
  display: block;
  height: 0.1rem;
  background: #ccc;
  width: 85%;
  border-radius: 0 0.1rem 0.1rem 0;
}

.compulsoryDiv .complete .tabDataChildren .barContainer em {
  color: #999;
  font-size: 15px;
}

.compulsoryDiv .complete .tabDataChildren:last-child {
  margin-bottom: 0rem;
}

.compulsoryDiv .noMore {
  text-align: center;
  font-size: 15px;
  height: 0.5rem;
  line-height: 0.5rem;
  color: #666;
  background: #f8f8f8;
}
